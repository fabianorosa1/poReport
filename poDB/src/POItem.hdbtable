COLUMN TABLE "PO.Item" (
     "POHeader.Id" INTEGER CS_INT,
	 "PRODUCT" NVARCHAR(10),
	 "QUANTITY" DECIMAL(13,	3) CS_FIXED,
	 "QUANTITYUNIT" NVARCHAR(3),
	 "DELIVERYDATE" DATE CS_DAYDATE) 
	 WITH ASSOCIATIONS( JOIN "PO.Header" AS "POHeader" ON "POHeader"."Id" = "POHeader.Id") 
	 UNLOAD PRIORITY 5 AUTO MERGE 