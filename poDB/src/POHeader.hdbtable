COLUMN TABLE "PO.Header" (
     "Id" INTEGER CS_INT GENERATED ALWAYS AS IDENTITY (NO CYCLE NO CACHE NO MAXVALUE NO MINVALUE START WITH 1000000000 INCREMENT BY 1) NOT NULL ,
	 "PARTNER" NVARCHAR(10),
	 "CURRENCY" NVARCHAR(5),
	 "GROSSAMOUNT" DECIMAL(15,2) CS_FIXED,
	 PRIMARY KEY ("Id")) 
WITH ASSOCIATIONS( JOIN "PO.Item" AS "Items" ON "Id" = "Id") 
UNLOAD PRIORITY 5 AUTO MERGE 